FROM
     "DAGMARA"."KSIEGOWOSC" KSIEGOWOSC
WHERE KSIEGOWOSC."DATA_TRANSAKCJI" BETWEEN $P{dateFrom} AND $P{dateTo}
order by 5, 4 desc]]>
