ELSE 'Zamówienia' END AS GRUPOWANIE,
     KSIEGOWOSC."KWOTA" AS KWOTA,
     (select sum(KSIEGOWOSC1."KWOTA") from "DAGMARA"."KSIEGOWOSC" KSIEGOWOSC1 WHERE KSIEGOWOSC1."IDZAMOWIENIA" IS NOT NULL) AS KWOTAZAMOWIENIA,
